set term png

set title mytitle
set termoption lw 2
set output outfile
set xrange [0:94]
set xlabel "Experiment Number"
set ylabel myylabel
#set yrange [0:100000]
plot file using 1:($2 < 1 ? 0 : log10($2)) title 'IGIA' with linespoints,file using 1:($3 < 1 ? 0 : log10($3)) title 'Red. Consts, Red. Formulas' with linespoints